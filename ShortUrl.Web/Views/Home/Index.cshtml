
@{
    ViewData["Title"] = "Generate";
}

<style>

    html,
    body {
        height: 100%;
    }

    body {
        display: -ms-flexbox;
        display: flex;
        -ms-flex-align: center;
        align-items: center;
        padding-top: 40px;
        padding-bottom: 40px;
        background-color: #f5f5f5;
    }

    .form-signin {
        width: 100%;
        max-width: 430px;
        padding: 15px;
        margin: auto;
        text-align: center;
    }

        .form-signin .checkbox {
            font-weight: 400;
        }

        .form-signin .form-control {
            position: relative;
            box-sizing: border-box;
            height: auto;
            padding: 10px;
            font-size: 16px;
        }

            .form-signin .form-control:focus {
                z-index: 2;
            }

        .form-signin input[type="email"] {
            margin-bottom: -1px;
            border-bottom-right-radius: 0;
            border-bottom-left-radius: 0;
        }

        .form-signin input[type="password"] {
            margin-bottom: 10px;
            border-top-left-radius: 0;
            border-top-right-radius: 0;
        }
</style>

<form class="form-signin" action="/confrimgenerate" method="post">
    <svg t="1585026334394" class="icon mb-4" viewBox="0 0 1024 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" class="mb-4" width="72" height="72"><path d="M512 0A513.536 513.536 0 0 0 0 512a513.536 513.536 0 0 0 512 512 513.536 513.536 0 0 0 512-512A513.536 513.536 0 0 0 512 0z m55.424 612.48l-98.176 98.176a109.184 109.184 0 0 1-153.6 0l-1.92-1.792a109.312 109.312 0 0 1 0-153.6L382.08 486.4a151.168 151.168 0 0 0 11.008 59.52l-44.416 44.16a59.008 59.008 0 0 0 0 83.456l1.792 1.792a59.008 59.008 0 0 0 83.456 0l98.176-98.176a58.88 58.88 0 0 0 0-83.328l-1.792-1.92a56.192 56.192 0 0 0-11.776-8.96l35.84-35.968a100.48 100.48 0 0 1 11.264 9.728l1.792 1.792a109.44 109.44 0 0 1 0 153.984z m143.232-143.232L641.92 537.6a151.552 151.552 0 0 0-11.008-59.52l44.416-44.416a59.008 59.008 0 0 0 0-83.456l-1.792-1.792a59.264 59.264 0 0 0-83.456 0l-98.176 98.176a59.008 59.008 0 0 0 0 83.456l1.792 1.792a56.192 56.192 0 0 0 11.776 8.96l-35.968 35.968a129.92 129.92 0 0 1-11.136-9.6l-1.792-1.792a109.312 109.312 0 0 1 0-153.6l98.176-98.176a109.184 109.184 0 0 1 153.6 0l1.92 1.92a109.184 109.184 0 0 1 0.384 153.728z" fill="#FF7B3E" p-id="609"></path></svg>
    <label for="inputUrl" class="sr-only">Email address</label>
    <input id="inputUrl" class="form-control" placeholder="请输入url" required autofocus name="url" type="url">
    <div class="mb-3"></div>
    <button class="btn btn-lg btn-primary btn-block" type="submit">生成</button>
</form>



<div class="my-3 p-3 bg-white rounded shadow-sm">
    <h6 class="border-bottom border-gray pb-2 mb-0">生成结果</h6>
    <div class="media text-muted pt-3">
        <svg class="bd-placeholder-img mr-2 rounded" width="32" height="32" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: 32x32"><title>Placeholder</title><rect width="100%" height="100%" fill="#007bff" /><text x="50%" y="50%" fill="#007bff" dy=".3em">32x32</text></svg>
        <p class="media-body pb-3 mb-0 small lh-125 border-gray">
            <strong class="d-block text-gray-dark">短链接</strong>
            <a href="@ViewBag.shortUrl" target="_blank">@ViewBag.shortUrl</a>
        </p>
    </div>
</div>
